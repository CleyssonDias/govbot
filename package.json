    "rou3": "0.7.10",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "typescript": "5.7.2",
    "@types/bun": "^1.2.10"
  },
  "imports": {
    "#env": [
      "./build/env.js"
    ],
    "#base": [
      "./build/discord/base/index.js"
    ],
    "#functions": [
      "./build/functions/index.js"
    ],
    "#database": [
      "./build/database/index.js"
    ],
    "#server": [
      "./build/server/index.js"
    ],
    "#menus": [
      "./build/menus/index.js"
    ],
    "#tools": [
      "./build/tools/index.js"
